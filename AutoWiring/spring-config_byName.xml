<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="emp1" class="com.hr.Employee" autowire="byName">
		<property name="empId" value="1001"/>
		<property name="name" value="Rahul Barve"/>
		<property name="sal" value="10000"/>	 	
	</bean>
	<bean name="contact" class="com.hr.Contact">
		<property name="email" value="rahulbarve1979@gmail.com"/>
		<property name="mobile" value="+918669887867"/>
	</bean>
	<bean name="address" class="com.hr.Address">
		<property name="city" value="Pune"/>
		<property name="state" value="MH"/>
		<property name="zip" value="411038"/>
	</bean>		
</beans>
