	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	
	<bean id="obj1" class="com.Employee" scope="prototype">
		<constructor-arg value="100"></constructor-arg>
		<constructor-arg value="Raj"></constructor-arg>
		<constructor-arg value="12000"></constructor-arg>
	</bean>
	
	<bean id="obj2" class="com.Employee">
		
	</bean>
	
	<bean id="obj3" class="com.Employee" scope="prototype">
		<constructor-arg value="Raj" index="1"></constructor-arg>
		<constructor-arg value="12000" index="2"></constructor-arg>
		<constructor-arg value="100" index="0"></constructor-arg>
	</bean>
	

